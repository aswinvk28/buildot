<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Bundle\CompanyBundle\Entity\AbuseReports" table="abuse_reports">
    <id name="reportId" type="integer" column="report_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="tableName" type="string" column="table_name" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="id" type="integer" column="id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="created" type="datetime" column="created" nullable="true"/>
  </entity>
</doctrine-mapping>
