<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Bundle\CompanyBundle\Entity\Countries" table="countries">
    <id name="countryid" type="integer" column="countryId">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="country" type="string" column="country" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="currency" type="string" column="currency" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="currencyName" type="string" column="currency_name" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="countryCode" type="string" column="country_code" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="countryLetter" type="string" column="country_letter" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="currencyDisplay" type="boolean" column="currency_display" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="selected" type="boolean" column="selected" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="display" type="boolean" column="display" nullable="true">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
